{
  "name": "react-simple-boilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate for react",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress",
    "build:dll": "cross-env NODE_ENV=production webpack --config webpack/dll.config.js",
    "build:prod": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --inline --config webpack/dev.config.js",
    "postinstall": "npm run build:dll",
    "prod": "npm run build:prod && node server/entry.js",
    "test": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerekshoe/react-simple-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "heroku",
    "boilerplate",
    "react-router"
  ],
  "author": "jerekshoe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jerekshoe/react-simple-boilerplate/issues"
  },
  "homepage": "https://github.com/jerekshoe/react-simple-boilerplate#readme",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.16.3",
    "bootstrap": "^3.3.7",
    "bootstrap-loader": "^2.0.0-beta.12",
    "bootstrap-sass": "^3.3.7",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.1",
    "node-sass": "^3.10.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.12.0",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "webpack-dev-server": "^2.1.0-beta.9"
  }
}
